package rules

//list any import classes here.
import com.sample.DroolsTest.*


//declare any global variables here
import pl.put.poznan.*;

rule "Znamy Odpowiedz"
	salience 10001
	when
		Fakt(klucz=="panstwo", $panstwo : tekst)
	then
		Fakt.oglos($panstwo);
		drools.halt();
end

rule "Albania"
	salience 10000
	when
		Fakt(klucz=="polozenie", tekst=="Europa poludniowa")
	then
		insert(new Fakt("panstwo", "Albania"));
end

